<div class="row">
<div class="text-center">
	<div class="col-md-6">
	<button type="button" class="btn btn-warning">NOT ADDED</button>
	<button type="button" class="btn btn-danger">ADDED/NOT COMPLETED</button>
	<button type="button" class="btn btn-success">COMPLETED</button>
	</div>
	<div class="col-md-1 col-md-push-4">
		<a class="list-inline-item" href="./checkMessages"><button type="button" class="btn btn-info" onclick=" ">Check Messages </button></a>
	</div>
</div>
</div>
<div class="aptsContainer">
{{!-- PHASE 1 --}}
<div class="phase1">
<h2><u><strong>Phase 1:</strong></u></h2>
{{#each infoP1}}
<div class="row">
<div class="text-center">
	<div class="row">
	<div class="col-md-6 col-md-push-2">
				
				<p class="aptname">
				<div class="aptDes" >Apt: {{apt}}, Bedrooms: {{bedrooms}}, Bathrooms: {{bathrooms}}</div>
				</p>

	</div>
	</div>
	<div class="row">
	<div class="col-md-1 col-md-push-1">
				<button type="button" class="btn btn-default" onclick="noturn({{@index}})">NO TURN</button>
	</div>
	<div class="col-md-6 col-md-push-1">
				<p><div class="needs">Needs:
					<button type="button" class="btn btn-{{paintBtn}} " onclick="addPaint({{@index}})">Paint</button>
					<button type="button" class="btn btn-{{maintenanceBtn}} " onclick="addMaintenance({{@index}})">Maintenence</button>
					<button type="button" class="btn btn-{{cleaningBtn}} " onclick="addCleaning({{@index}})">Cleaning</button>
				</div></p>
	</div>
	</div>
	<div class="row">
	<div class="col-md-6">
				<div class="notes">
				<p><div class="noteWord">Notes:</div> {{notes}}</p>
				</div>
	</div>
	</div>
	<div class="row">
	<div class="col-md-10">
		<p> <b>------------------------------------------------------------------------------------------------------------------------------------</b> </p>			
	</div>
	</div>
</div>
</div>
{{/each}}
</div>

