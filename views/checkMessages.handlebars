<div class="msgs">
<h2><u><strong>Messages:</strong></u></h2>

{{#each infomessage}}
<div class="row">
<div class="text-center">
	<div class="row">
	<div class="col-md-10">
				
				<p class="msgname">
				<div class="msgDes" ><b>Name:</b> {{name}} <b>Contact Info:</b> {{emailPhone}}</div>
				</p>

	</div>
	</div>
	<div class="row">
	<div class="col-md-10">
				<p><b>Message:</b> {{message}}</p>
	</div>
	</div>
	<div class="row">
	<div class="col-md-10">
		<p> <b>------------------------------------------------------------------------------------------------------------------------------------</b> </p>			
	</div>
	</div>
</div>
</div>
{{/each}}
</div>
